{
  "name": "@atlassian/github-for-jira",
  "version": "0.0.0",
  "description": "Github integration for Atlassian Jira",
  "repository": "https://github.com/atlassian/github-for-jira.git",
  "engines": {
    "node": ">= 14.16 <15",
    "npm": "^6.12.0"
  },
  "main": "src/main",
  "scripts": {
    "prestart": "ts-node env.ts",
    "start": "run-p start:main start:worker",
    "start:main": "ts-node-dev --inspect=0.0.0.0:9229 --respawn --transpile-only --rs src/main",
    "start:worker": "ts-node-dev --inspect=0.0.0.0:9230 --respawn --transpile-only --rs src/worker",
    "start:production": "run-p start:main:production start:worker:production",
    "start:main:production": "node .",
    "start:worker:production": "node src/worker",
    "clean": "rimraf coverage build tmp src/*.js src/*.js.map",
    "build": "tsc -p ./tsconfig.json",
    "build:release": "tsc -p ./tsconfig.release.json",
    "build:watch": "tsc -w -p ./tsconfig.json",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "jest --runInBand --forceExit --coverage",
    "test:watch": "jest --runInBand --watchAll",
    "db": "run-s db:create db:migrate",
    "db:create": "run-p db:create:*",
    "db:create:dev": "dotenv -- sequelize db:create || exit 0",
    "db:create:test": "dotenv -e .env.test -- sequelize db:create --env test || exit 0",
    "db:migrate": "run-p db:migrate:*",
    "db:migrate:dev": "dotenv -- sequelize db:migrate",
    "db:migrate:test": "dotenv -e .env.test -- sequelize db:migrate --env test",
    "db:dev": "run-s db:create:dev db:migrate:dev",
    "db:test": "run-s db:create:test db:migrate:test",
    "prepare": "husky install",
    "precommit": "run-p lint build"
  },
  "dependencies": {
    "@atlaskit/css-reset": "^2.0.5",
    "@atlaskit/reduced-ui-pack": "^8.19.0",
    "@octokit/auth-app": "^2.6.0",
    "@sentry/node": "^6.8.0",
    "@types/bull": "^3.12.2",
    "@types/bunyan": "^1.8.6",
    "@types/cache-manager": "^3.4.0",
    "@types/cookie-session": "^2.0.42",
    "@types/csurf": "^1.11.1",
    "@types/express": "^4.17.11",
    "@types/express-rate-limit": "^5.1.1",
    "@types/hbs": "^4.0.1",
    "@types/jest": "~26.0.23",
    "@types/lodash": "^4.14.170",
    "@types/moo": "^0.5.4",
    "@types/newrelic": "^6.2.0",
    "@types/node": "~14.14.44",
    "@types/rate-limit-redis": "^1.7.1",
    "@types/supertest": "^2.0.11",
    "atlassian-jwt": "^0.1.5",
    "axios": "^0.21.1",
    "body-parser": "^1.18.3",
    "bottleneck": "^2.19.5",
    "bull": "^3.12.1",
    "bunyan": "^1.8.15",
    "cookie-session": "^2.0.0-rc.1",
    "csurf": "^1.11.0",
    "date-fns": "^1.29.0",
    "express": "^4.17.1",
    "express-rate-limit": "^5.1.3",
    "express-sslify": "^1.2.0",
    "express-validator": "^6.4.1",
    "hbs": "^4.1.2",
    "helmet": "^3.21.2",
    "hot-shots": "^8.3.2",
    "ioredis": "^4.16.3",
    "jira-issue-key-parser": "0.0.11",
    "lodash": "^4.17.21",
    "moment": "^2.24.0",
    "moo": "^0.5.0",
    "pg": "^8.6.0",
    "primer": "^10.6.0",
    "probot": "^9.11.3",
    "rate-limit-redis": "^1.7.0",
    "sequelize": "^5.21.7",
    "sequelize-encrypted": "^1.0.0",
    "throng": "^4.0.0",
    "tslib": "~2.2.0",
    "typescript": "~4.2.4",
    "https-proxy-agent": "^5.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "~4.23.0",
    "@typescript-eslint/parser": "~4.23.0",
    "dotenv-cli": "^4.0.0",
    "eslint": "~7.26.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "~8.3.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "~24.3.6",
    "eslint-plugin-jsdoc": "^16.1.1",
    "eslint-plugin-no-only-tests": "^2.3.1",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "jest-diff": "^25.4.0",
    "keygrip": "^1.0.2",
    "nock": "^13.0.11",
    "npm-run-all": "^4.1.5",
    "prettier": "~2.3.0",
    "rimraf": "~3.0.2",
    "sequelize-cli": "^6.2.0",
    "smee-client": "^1.2.2",
    "supertest": "^4.0.2",
    "ts-jest": "~26.5.6",
    "ts-node": "^10.0.0",
    "ts-node-dev": "^1.1.6"
  },
  "volta": {
    "node": "14.17.0"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "<rootDir>/test/setup/setup.ts"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "maxConcurrency": 1,
    "maxWorkers": 1
  }
}
